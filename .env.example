# P0rt Environment Configuration
# Copiez ce fichier vers .env et modifiez les valeurs selon vos besoins

# API Key pour l'authentification (optionnel)
P0RT_API_KEY=your-secret-api-key-here

# Mode verbose pour les logs
P0RT_VERBOSE=false

# Configuration Redis (si vous utilisez Redis storage)
REDIS_PASSWORD=p0rt_redis_secure_password

# Domain configuration
P0RT_DOMAIN=p0rt.xyz

# Network configuration
P0RT_SSH_PORT=22
P0RT_HTTP_PORT=80

# Security settings
P0RT_BAN_DURATION=24h
P0RT_MAX_CONNECTIONS=100